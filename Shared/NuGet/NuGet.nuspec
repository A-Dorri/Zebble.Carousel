<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2012/06/nuspec.xsd">
  <metadata>
    <id>Zebble.Plugin.Carousel</id>
    <version>4.5.263</version>
    <title>Carousel for Zebble (Xamarin)</title>
    <description>A carousel plugin for Zebble allowing the user to swipe between different views.</description>
    <iconUrl>https://raw.githubusercontent.com/Geeksltd/Zebble.Carousel/master/Shared/Icon.png</iconUrl>
    <authors>Mobile App Technologies Ltd</authors>
    <owners>Mobile App Technologies Ltd</owners>
    <copyright>Copyright ©2017 | Mobile App Technologies Ltd</copyright>
    <dependencies>
      <group targetFramework="MonoAndroid">
        <dependency id="Zebble" version="4.1.285"></dependency>
      </group>
      <group targetFramework="Xamarin.iOS10">
        <dependency id="Zebble" version="4.1.285"></dependency>
      </group>
    </dependencies>
  </metadata>
  <files>
    <file src="..\..\lib\Zebble.Carousel.iOS.*" target="lib\Xamarin.iOS10\" />
    <file src="..\..\lib\Zebble.Carousel.Android.*" target="lib\MonoAndroid10\" />
    <file src="..\..\lib\Zebble.Carousel.UWP.*" target="lib\UAP10\" />
  </files>
</package>